# Local/private config goes in the include
[include]
    path = ~/.gitconfig.local
[hub]
    protocol = https
[alias]
    co = checkout
    cob = checkout -b
    count   = !git shortlog -sn
    refresh = !git fetch --all && git pull --all
    cam = !git add . && git commit -m
    st = status
    wip = !git add . && git commit -m "wip"  && git push
[color]
    diff = auto
    status = auto
    branch = auto
    ui = true
[apply]
    whitespace = nowarn
[mergetool]
    keepBackup = false
[difftool]
    prompt = false
[help]
    autocorrect = 1
[push]
    # See `git help config` (search for push.default)
    # for more information on different options of the below setting.
    #
    # Setting to git 2.0 default to suppress warning message
    default = simple
[commit]
	template = ~/.gitcommitmessage
[user]
	email = cj@ponti.io
	name = Charlie
