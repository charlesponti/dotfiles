{
	"[csv]": {
		"editor.inlayHints.maximumLength": 0
	},
	"[go]": {
		"editor.defaultFormatter": "golang.go"
	},
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[markdown]": {
		"cSpell.advanced.feature.useReferenceProviderRemove": "/^#+\\s/",
		"cSpell.advanced.feature.useReferenceProviderWithRename": true,
		"cSpell.fixSpellingWithRenameProvider": true,
		"diffEditor.ignoreTrimWhitespace": false,
		"editor.formatOnSave": false,
		"editor.quickSuggestions": {
			"comments": "off",
			"other": "off",
			"strings": "off"
		},
		"editor.unicodeHighlight.ambiguousCharacters": false,
		"editor.unicodeHighlight.invisibleCharacters": false,
		"editor.wordWrap": "on"
	},
	"[python]": {
		"editor.defaultFormatter": "charliermarsh.ruff",
		"editor.formatOnSave": true
	},
	"[search-result]": {
		"editor.lineNumbers": "on"
	},
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[xml]": {
		"editor.defaultFormatter": "redhat.vscode-xml"
	},
	"bicep.enableSurveys": false,
	"cSpell.ignorePaths": [
		"package-lock.json",
		"node_modules",
		"vscode-extension",
		".git/{info,lfs,logs,refs,objects}/**",
		".git/{index,*refs,*HEAD}",
		".vscode",
		".vscode-insiders",
		".zsh_history"
	],
	"cSpell.userWords": [
		"charliermarsh",
		"codesearch",
		"firestore",
		"geminicodeassist",
		"Powerline",
		"Pylance",
		"Rewardful",
		"rgba",
		"supabase",
		"venv",
		"vsintellicode"
	],
	"debug.console.fontFamily": "Geist Mono",
	"diffEditor.codeLens": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"diffEditor.renderSideBySide": false,
	"editor.accessibilitySupport": "off",
	"editor.codeLensFontFamily": "Geist Mono",
	"editor.defaultFormatter": "biomejs.biome",
	"editor.fontFamily": "Geist Mono",
	"editor.fontSize": 11,
	"editor.fontWeight": "350",
	"editor.formatOnSave": true,
	"editor.formatOnSaveMode": "file",
	"editor.inlayHints.fontFamily": "Geist Mono",
	"editor.inlineSuggest.enabled": true,
	"editor.inlineSuggest.fontFamily": "Geist Mono",
	"editor.inlineSuggest.suppressSuggestions": true,
	"editor.largeFileOptimizations": true,
	"editor.lineHeight": 2,
	"editor.linkedEditing": true,
	"editor.suggest.showWords": false,
	"editor.suggestSelection": "first",
	"editor.tabSize": 2,
	"editor.unicodeHighlight.ambiguousCharacters": false,
	"eslint.format.enable": true,
	"eslint.lintTask.enable": true,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.patterns": {
		"*.cts": "${capture}.typegen.ts",
		"*.mts": "${capture}.typegen.ts",
		"*.ts": "${capture}.typegen.ts",
		"*.tsx": "${capture}.typegen.ts",
		"Cargo.toml": "Cargo.lock"
	},
	"git.autofetch": true,
	"git.blame.editorDecoration.enabled": true,
	"git.blame.statusBarItem.enabled": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"git.openRepositoryInParentFolders": "always",
	"git.path": "/usr/bin/git",
	"github.copilot.chat.codesearch.enabled": true,
	"github.copilot.selectedCompletionModel": "",
	"githubPullRequests.fileListLayout": "tree",
	"githubPullRequests.pullBranch": "always",
	"githubPullRequests.pushBranch": "always",
	"go.toolsManagement.autoUpdate": true,
	"javascript.preferences.quoteStyle": "single",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"makefile.configureOnOpen": true,
	"markdown.preview.fontFamily": "Geist Mono",
	"mcp": {
		"servers": {
			"hominem": {
				"args": [
					"node",
					"/Users/charlesponti/Developer/hominem/apps/mcp/build/index.js"
				],
				"command": "DATABASE_URL=postgresql://postgres:postgres@postgres:4432/hominem",
				"type": "stdio"
			}
		}
	},
	"python.analysis.autoImportCompletions": true,
	"python.analysis.completeFunctionParens": true,
	"python.analysis.languageServerMode": "full",
	"python.defaultInterpreterPath": "",
	"python.experiments.enabled": false,
	"python.languageServer": "Pylance",
	"python.terminal.activateEnvInCurrentTerminal": true,
	"python.venvPath": "${workspaceFolder}/.venv",
	"redhat.telemetry.enabled": false,
	"remote.defaultExtensionsIfInstalledLocally": [
		"GitHub.copilot",
		"GitHub.copilot-chat",
		"GitHub.vscode-pull-request-github"
	],
	"scm.defaultViewMode": "tree",
	"scss.showErrors": true,
	"search.defaultViewMode": "tree",
	"security.promptForLocalFileProtocolHandling": false,
	"security.workspace.trust.untrustedFiles": "open",
	"svelte.enable-ts-plugin": true,
	"terminal.integrated.defaultProfile.osx": "zsh",
	"terminal.integrated.env.osx": {
		"CW_NEW_SESSION": "1",
		"Q_NEW_SESSION": "1"
	},
	"terminal.integrated.fontFamily": "Geist Mono",
	"terminal.integrated.fontLigatures.featureSettings": "\"calt\" on, \"liga\" on",
	"terminal.integrated.fontSize": 12,
	"terminal.integrated.fontWeightBold": "400",
	"typescript.preferences.quoteStyle": "single",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"vs-kubernetes": {
		"vscode-kubernetes.helm-path-mac": "/Users/charlesponti/.vs-kubernetes/tools/helm/darwin-arm64/helm",
		"vscode-kubernetes.minikube-path-mac": "/Users/charlesponti/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
	},
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"vsintellicode.python.completionsEnabled": true,
	"window.commandCenter": true,
	"window.newWindowDimensions": "inherit",
	"workbench.colorTheme": "Catppuccin Latte",
	"workbench.editor.enablePreview": false,
	"workbench.iconTheme": "catppuccin-latte",
	"workbench.sideBar.location": "right",
	"workbench.startupEditor": "none",
	"docker.extension.enableComposeLanguageServer": false,
	"amp.commands.allowlist": [
		"timeout"
	],
	"[postcss]": {
		"editor.defaultFormatter": "stylelint.vscode-stylelint"
	}
}
